this:configuration1 mexcore:ExperimentConfiguration prov:Entity ;
    prov:used this:model1 ;
    prov:wasStartedBy this:khan_task3 .

this:configuration2 mexcore:ExperimentConfiguration prov:Entity ;
    prov:used this:model2 ;
    prov:wasStartedBy this:khan_task3 .

this:dataset rdfs:label "Sentiment-Classification" ;
    mexcore:Dataset prov:Entity ;
    dct:landingPage "https://github.com/SmartDataAnalytics/MA-INF-4222-NLP-Lab/blob/master/2018_SoSe/exercises/dataset_sentiment.csv" .

this:execution1 mexcore:ExecutionOverall prov:Entity ;
    prov:generated this:performance_measures1 ;
    prov:used this:hyerparameter_model1,
        this:model1,
        this:test .

this:execution2 mexcore:ExecutionOverall prov:Entity ;
    prov:generated this:performance_measures2 ;
    prov:used this:hyerparameter_model2,
        this:model2,
        this:test .

this:hyerparameter_model1 mexalgo:HyperParameterCollection prov:Entity ;
    prov:hadMember this:hyerparameter1,
        this:hyerparameter2,
        this:hyerparameter3 .

this:hyerparameter_model2 rdfs:label "HyperParameterCollection" ;
    mexalgo:HyperParameterCollection prov:Entity ;
    prov:hadMember this:hyerparameter4,
        this:hyerparameter5,
        this:hyerparameter6 .

this:performance_measures1 mexcore:PerformanceMeasure prov:Entity ;
    mexperf:accuracy "0.85"^^xsd:float ;
    mexperf:neg_accuracy "0.95"^^xsd:float,
        "0.97"^^xsd:float ;
    mexperf:pos_accuracy "0.4"^^xsd:float,
        "0.45"^^xsd:float ;
    mexperf:score "0.36"^^xsd:float ;
    prov:wasGeneratedBy this:execution1 .

this:performance_measures2 mexcore:PerformanceMeasure prov:Entity ;
    mexperf:accuracy "0.85"^^xsd:float ;
    mexperf:score "0.36"^^xsd:float ;
    prov:wasGeneratedBy this:execution2 .

this:hyerparameter1 rdfs:label "HyperParameterCollection",
        "embedding_dimension" ;
    mexalgo:HyperParameter prov:Entity ;
    dct:identifier "embedding_dimension" ;
    prov:value 128 .

this:hyerparameter2 rdfs:label "dropout" ;
    mexalgo:HyperParameter prov:Entity ;
    dct:identifier "dropout" ;
    prov:value "0.1"^^xsd:float .

this:hyerparameter3 rdfs:label "recurrent_dropout" ;
    mexalgo:HyperParameter prov:Entity ;
    dct:identifier "recurrent_dropout" ;
    prov:value "0.1"^^xsd:float .

this:hyerparameter4 rdfs:label "embedding_dimension" ;
    mexalgo:HyperParameter prov:Entity ;
    dct:identifier "embedding_dimension" ;
    prov:value 128 .

this:hyerparameter5 rdfs:label "convolution_filters" ;
    mexalgo:HyperParameter prov:Entity ;
    dct:identifier "convolution_filters" ;
    prov:value 12 .

this:hyerparameter6 rdfs:label "filter_size" ;
    mexalgo:HyperParameter prov:Entity ;
    dct:identifier "filter_size" ;
    prov:value 3 .

this:khan_task3 rdfs:label "2255383" ;
    mexcore:ApplicationContext prov:Entity ;
    mexcore:Experiment prov:Entity ;
    dct:date "2018-05-15"^^xsd:date ;
    foaf:givenName "Asif" ;
    foaf:mbox "mak4086@gmail.com" .

this:model1 rdfs:label "LSTM" ;
    mexalgo:Algorithm prov:Entity ;
    mexalgo:hasHyperParameter this:hyerparameter1,
        this:hyerparameter2,
        this:hyerparameter3 ;
